
<h1>Add new user!</h1>

<div class="container">
  <%= form_for(@user) do |f| %>
    <div class="form-group">
      <%= f.label :name %>
      <%= f.text_field :name, class: 'form-control' %>
    </div>

    <div class="form-group">
      <%= f.label :github_Name %>
      <%= f.text_field :github, class: 'form-control' %>
    </div>

     <div class="form-group">
      <%= f.label :job %>
      <%= f.text_field :job, class: 'form-control' %>
    </div>

    <%= f.submit class: 'btn btn-primary' %>
  <% end %>
</div>
